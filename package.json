{
  "name": "nuxt-devtools",
  "type": "module",
  "version": "0.0.0-alpha.0",
  "packageManager": "pnpm@7.1.0",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/nuxt-devtools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/nuxt-devtools.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/nuxt-devtools/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "nr build",
    "build": "nr build:module && nr build:client",
    "build:module": "nuxt-module-build",
    "build:client": "nuxi generate client",
    "play": "nuxi dev client",
    "dev": "nr play",
    "lint": "eslint .",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepare": "nuxi prepare client"
  },
  "dependencies": {
    "@nuxt/kit": "3.0.0-rc.3",
    "birpc": "^0.2.3",
    "flatted": "^3.2.5",
    "h3": "^0.7.8",
    "launch-editor": "^2.4.0",
    "pathe": "^0.3.0",
    "sirv": "^2.0.2",
    "tinyws": "^0.1.0",
    "unimport": "^0.2.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.41",
    "@types/ws": "^8.5.3",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "nuxt": "^3.0.0-rc.3",
    "nuxt-devtools": "workspace:*",
    "shiki": "^0.10.1",
    "typescript": "^4.7.3",
    "ws": "^8.7.0"
  },
  "pnpm": {
    "overrides": {
      "@unocss/nuxt": "^0.38.1"
    }
  }
}
